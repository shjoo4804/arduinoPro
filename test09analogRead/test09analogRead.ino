#include <stdio.h>
#define TIME 100
#define LED 3 

int reg = 0;
void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  Serial.println("setup()...");
}

void loop() {
  Serial.println("loop()...");
  reg = analogRead(A0);
  Serial.println(reg); // 0~1023
  Serial.println(map(reg,0,1023,0,255)); // 0~1023 >>mapping>> 0~255
  analogWrite(LED,map(reg,0,1023,0,255));
  delay(TIME);
}
